#include <iostream>
#include <cmath>
#define pi 3.14159
using namespace std;

float r, h, V;
int main(){
    cout<<"Tell me the radius of the cylinder: ";
    cin>>r;
    cout<<"Tell me the height of the cylinder: ";
    cin>>h;
    
    V=(pi)*(pow(r,2))*(h);
    
    cout<<"The Volume of the cylinder is: "<<V<<endl;
    
}
